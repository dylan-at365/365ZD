<!-- Start User Profile Template -->
<div class="container">
    <div class="row profile_info">
        <div class="col-12 profile_info-avatar">
            <img class="user-avatar" src="{{user.avatar_url}}" alt="User Avatar" />
        </div>

        <div class="col-12 profile_info-about">
            <div class="profile_info-about_name">
                {{#if user.url}}
                    <a href="{{user.url}}" target="_zendesk_lotus" title="{{t 'open_user_in_support'}}">{{user.name}}</a>
                {{else}}
                    {{user.name}}
                {{/if}}

                <a role="button" class="profile_info-about_name-edit" title="Opens a dialog" data-action="edit-profile" href="#">
                    <i class="ph-pencil-line-bold"></i>
                </a>
            </div>

            <ul class="profile_info-about_name-stats">
                <li>
                    {{t 'member_since'}}</span>
                    {{#if member_since}}
                        {{date member_since format='medium'}}
                    {{else}}
                        {{t 'no_activity_yet'}}
                    {{/if}}
                </li>
                <li>
                    {{t 'last_activity'}}
                    {{#if last_activity_at}}
                        {{date last_activity_at timeago=true}}
                    {{else}}
                        {{t 'no_activity_yet'}}
                    {{/if}}
                </li>
                <li>
                    {{t 'votes'}}
                    {{vote_count}}
                </li>
                <li>
                    Saved Articles
                    {{subscription_count}}
                </li>

                {{#if description}}
                    <li class="profile_info-about_desc">{{description}}</li>
                {{/if}}
            </ul>

            <div class="profile_info-about_name-recent">
                {{recent_articles}}
            </div>

        </div>		
	</div>

